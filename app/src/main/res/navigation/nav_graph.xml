<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/QrCodeReaderFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="idv.hsu.authenticator.FirstFragment"
        android:label="@string/fragment_label_first"
        tools:layout="@layout/fragment_first">

        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="idv.hsu.authenticator.SecondFragment"
        android:label="@string/fragment_label_second"
        tools:layout="@layout/fragment_second">

        <action
            android:id="@+id/action_SecondFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
    </fragment>
    <fragment
        android:id="@+id/QrCodeReaderFragment"
        android:name="idv.hsu.authenticator.QrCodeReaderFragment"
        android:label="@string/fragment_label_qr_code_reader"
        tools:layout="@layout/fragment_qr_code_reader" />
</navigation>